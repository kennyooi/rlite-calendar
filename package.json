{
  "name": "rlite-calendar",
  "version": "0.1.0",
  "description": "A lightweight and customizable React.js calendar component.",
  "keywords": [
    "react",
    "calendar",
    "datepicker",
    "lightweight"
  ],
  "author": "KennyOoi <kenny@qutebox.com>",
  "license": "MIT",
  "scripts": {
    "build:demo": "cd demo && yarn build",
    "prebuild": "rm -rf dist && rm -rf src && cp -r demo/src/main/. src/",
    "build": "webpack --mode production",
    "build:report": "webpack --mode development --config webpack.config.dev.js",
    "serve": "cd demo && yarn start"
  },
  "main": "./dist/rlite-calendar.umd.js",
  "module": "./dist/rlite-calendar.common.js",
  "unpkg": "./dist/rlite-calendar.umd.min.js",
  "browser": {
    "./sfc": "src/Calendar.js"
  },
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "classnames": "^2.2.6",
    "date-fns": "^2.14.0"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "babel-loader": "^8.1.0",
    "cross-env": "^6.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11"
  }
}
